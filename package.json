{
  "name": "prisma-backend",
  "version": "1.0.0",
  "description": "TS project with Prisma ORM",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "prismix": "npx prismix && prisma format",
    "dev": "yarn prismix &&  ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "build": "rimraf dist && npx tsc -p tsconfig-build.json",
    "lint": "npx eslint . --no-error-on-unmatched-pattern",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "test": "test",
    "migrate": "yarn prismix && prisma migrate dev --name ",
    "migrate:init": "yarn migrate init",
    "generate": "yarn prismix && prisma generate",
    "studio": "yarn prismix && prisma studio"
  },
  "author": "Marcio T Rodrigues",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "module-alias": "^2.2.2",
    "pg-mem": "^2.6.4",
    "prisma": "^4.8.0",
    "prismix": "^1.0.22",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "prisma": {
    "schema": "src/infra/repos/postgres/schema/schema.prisma"
  }
}
